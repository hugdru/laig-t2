<SCENE>
  <INITIALS>
    <frustum near="0" far="150" />
    <translation x="10" y="0" z="5" />
    <rotation axis="x" angle="45" />
    <rotation axis="y" angle="45" />
    <rotation axis="z" angle="2" />
    <scale sx="1" sy="1" sz="1" />
    <reference length="5" />
  </INITIALS>
  <ILLUMINATION>
    <ambient r="0" g="0" b="0" a="1" />
    <background r="0" g="0.5" b="0" a="1" />
  </ILLUMINATION>
  <LIGHTS>
    <LIGHT id="light1">
      <enable value="1" />
      <position x="7" y="7" z="7" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <specular r="0.3" g="0.3" b="0.3" a="1" />
    </LIGHT>
    <LIGHT id="light2">
      <enable value="0" />
      <position x="10" y="10" z="10" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <specular r="0.3" g="0.3" b="0.3" a="1" />
    </LIGHT>
  </LIGHTS>
  <TEXTURES>
    <TEXTURE id="wall">
      <file path="textures/wall.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
    <TEXTURE id="floor">
      <file path="textures/floor.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
    <TEXTURE id="sphere">
      <file path="textures/sphere.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
    <TEXTURE id="cube">
      <file path="textures/cube.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
  </TEXTURES>
  <MATERIALS>
    <MATERIAL id="default">
      <shininess value="5" />
      <specular r="0.3" g="0.3" b="0.3" a="1" />
      <diffuse r="0.3" g="0.3" b="0.3" a="1" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />
    </MATERIAL>
    <MATERIAL id="red">
      <shininess value="5" />
      <specular r="1" g="0" b="0" a="1" />
      <diffuse r="1" g="0" b="0" a="1" />
      <ambient r="1" g="0" b="0" a="1" />
      <emission r="1" g="0" b="0" a="1" />
    </MATERIAL>
    <MATERIAL id="green">
      <shininess value="5" />
      <specular r="0" g="1" b="0" a="1" />
      <diffuse r="0" g="1" b="0" a="1" />
      <ambient r="0" g="1" b="0" a="1" />
      <emission r="0" g="1" b="0" a="1" />
    </MATERIAL>
    <MATERIAL id="blue">
      <shininess value="5" />
      <specular r="0" g="0" b="1" a="1" />
      <diffuse r="0" g="0" b="1" a="1" />
      <ambient r="0" g="0" b="1" a="1" />
      <emission r="0" g="0" b="1" a="1" />
    </MATERIAL>
  </MATERIALS>
  <LEAVES>
    <LEAF id="walls" type="rectangle" args="0 5 10 0" />
    <LEAF id="floor1" type="rectangle" args="0 10 10 0" />
    <LEAF id="square" type="rectangle" args="0 1 1 0" />
    <LEAF id="sphere1" type="sphere" args="1 25 25" />
  </LEAVES>
  <NODES>
    <ROOT id="scene" />
    <NODE id="scene">
      <MATERIAL id="default" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <DESCENDANT id="left_wall" />
        <DESCENDANT id="right_wall" />
        <DESCENDANT id="floor" />
        <DESCENDANT id="cube" />
        <DESCENDANT id="sphere" />
      </DESCENDANTS>
    </NODE>
    <NODE id="floor">
      <MATERIAL id="null" />
      <TEXTURE id="floor" />
      <TRANSLATION x="0" y="0" z="10" />
      <ROTATION axis="x" angle="-90" />
      <DESCENDANTS>
        <DESCENDANT id="floor1" />
      </DESCENDANTS>
    </NODE>
    <NODE id="left_wall">
      <MATERIAL id="null" />
      <TEXTURE id="wall" />
      <TRANSLATION x="0" y="0" z="10" />
      <ROTATION axis="y" angle="90" />
      <DESCENDANTS>
        <DESCENDANT id="walls" />
      </DESCENDANTS>
    </NODE>
    <NODE id="right_wall">
      <MATERIAL id="null" />
      <TEXTURE id="wall" />
      <DESCENDANTS>
        <DESCENDANT id="walls" />
      </DESCENDANTS>
    </NODE>
    <NODE id="x_wall">
      <MATERIAL id="null" />
      <TEXTURE id="wall" />
      <DESCENDANTS>
        <DESCENDANT id="walls" />
      </DESCENDANTS>
    </NODE>
    <NODE id="cube">
      <MATERIAL id="red" />
      <TEXTURE id="cube" />
      <DESCENDANTS>
        <DESCENDANT id="base_square" />
        <DESCENDANT id="top_square" />
        <DESCENDANT id="left_square" />
        <DESCENDANT id="right_square" />
        <DESCENDANT id="front_square" />
        <DESCENDANT id="back_square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="front_square">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="3" y="0" z="3" />
      <ROTATION axis="y" angle="90" />
      <DESCENDANTS>
        <DESCENDANT id="square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="left_square">
      <MATERIAL id="green" />
      <TEXTURE id="null" />
      <TRANSLATION x="2" y="0" z="3" />
      <DESCENDANTS>
        <DESCENDANT id="square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="right_square">
      <MATERIAL id="green" />
      <TEXTURE id="null" />
      <TRANSLATION x="3" y="0" z="2" />
      <ROTATION axis="y" angle="180" />
      <DESCENDANTS>
        <DESCENDANT id="square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="back_square">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="2" y="0" z="2" />
      <ROTATION axis="y" angle="-90" />
      <DESCENDANTS>
        <DESCENDANT id="square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="top_square">
      <MATERIAL id="blue" />
      <TEXTURE id="null" />
      <TRANSLATION x="2" y="1" z="3" />
      <ROTATION axis="x" angle="-90" />
      <DESCENDANTS>
        <DESCENDANT id="square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="base_square">
      <MATERIAL id="blue" />
      <TEXTURE id="null" />
      <TRANSLATION x="2" y="0" z="2" />
      <ROTATION axis="x" angle="90" />
      <DESCENDANTS>
        <DESCENDANT id="square" />
      </DESCENDANTS>
    </NODE>
    <NODE id="sphere">
      <MATERIAL id="null" />
      <TEXTURE id="sphere" />
      <TRANSLATION x="4" y="0.5" z="4" />
      <DESCENDANTS>
        <DESCENDANT id="sphere1" />
      </DESCENDANTS>
    </NODE>
  </NODES>
</SCENE>
